    <div id="mask"></div>

    <section class="interface">
       <section id="materias" >
       


            <center><h1>Escolha a matéria desejada.</h1></center>
            <center><h2 style="color:white;"><%= Materium.pessoa(User.search.to_s()) %></h2></center>
         
         
