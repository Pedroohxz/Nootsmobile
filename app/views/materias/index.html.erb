    </center>
            </ul>
<div>

        </section>


      
       


        <center>
        <div id="pop-up">
        <div id="dentro">
        
            <center>
                <h2>Adicionar Nota</h2>
                
                
            </center>
        
        <center>
             
            <p>
                <label id="gap">
                  <input class="with-gap" name="group3" value="1"type="radio" id="one"/>
                  <span class="cyan-text text-accent-3">1º Bimestre</span>
        
                </label>
              </p>
        
              <p>
                <label id="gap2">
                  <input class="with-gap" name="group3" type="radio" value="2" id="one2"/>
                  <span class="cyan-text text-accent-3">2º Bimestre</span>
        
                </label>
              </p>
        
              <br>
              <p style="margin-left: 7px;">
                <label id="gap3">
                  <input class="with-gap" name="group3" value="3" type="radio" id="one3">
                  <span class="cyan-text text-accent-3">3º Bimestre</span>
        
                </label>
              </p>
        
