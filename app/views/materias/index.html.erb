                  <span class="cyan-text text-accent-3">4º Bimestre</span>
        
                </label>
              </p>
        
        
        </center>
        
        <center><div id="line_bottom"></div></center>
        
        
       
        
        
                <%= form_for(@noot) do |form| %>
        
            <div class="field1">
                <%= form.label :materia %>
                </br>
                <%= form.text_field :materia, id: :noot_materia, autocomplete: 'off' %>
                </br>
            </div>
        </br>
            <div class="field2">
                <%= form.label :nota_mensal %>
                </br>
                <%= form.number_field :nota_mensal, id: :noot_nota_mensal, autocomplete: 'off', step: :any %>
                </br>
            </div>
            <div class="field3">
                <%= form.label :nota_bimestral %>
                </br>
                <%= form.number_field :nota_bimestral, id: :noot_nota_bimestral, autocomplete: 'off', step: :any %>
                </br>
            </div>
        
            <div class="field5">
                <%= form.label :bimestre %>
                </br>
                <%= form.text_field :bimestre, id: :noot_bimestre, autocomplete: 'off' %>
                </br>
            </div>
        </br>
        
                <%= form.submit :Salvar, class: "btn waves-effect waves-light" %>
        
        
           

                </div>  
